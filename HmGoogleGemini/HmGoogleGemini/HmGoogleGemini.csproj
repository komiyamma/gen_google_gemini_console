<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net8.0-windows</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<AssemblyVersion>1.1.1.4</AssemblyVersion>
		<FileVersion>1.1.1.4</FileVersion>
		<Version>1.1.1.4</Version>
		<Authors>Akitsugu Komiyama</Authors>
		<Copyright>2024 @ $(Authors)</Copyright>
	</PropertyGroup>
	
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DebugType>none</DebugType>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DebugType>none</DebugType>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Google.Cloud.AIPlatform.V1" Version="3.1.0" />
	</ItemGroup>

</Project>
